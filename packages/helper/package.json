{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "http",
    "request",
    "XMLHttpRequest",
    "fetch",
    "https",
    "middleware",
    "interceptor"
  ],
  "scripts": {
    "start": "tsdx watch --verbose",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --format cjs,esm,umd",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "@prequest/helper",
  "author": "xdoer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdoer/PreQuest.git"
  },
  "bugs": {
    "url": "https://github.com/xdoer/PreQuest/issues"
  },
  "homepage": "https://github.com/xdoer/PreQuest#readme",
  "module": "dist/helper.esm.js",
  "size-limit": [
    {
      "path": "dist/helper.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/helper.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@prequest/types": "^0.1.7",
    "@prequest/utils": "^0.1.7",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/qs": "^6.9.6",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "gitHead": "128926b6d6d65f019d1f2c05cc23fa3e729192e9"
